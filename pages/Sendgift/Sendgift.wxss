/* 开屏 */
.headertop{
  width: 750rpx;
  height: 1000rpx;

 z-index: 9999;
}
.BigBox {
    width: 100%;
    height: 100%;
    background: transparent;
    overflow-y: hidden;
}
    .snow {
        width: 100%;
        height: 100%;
        overflow:hidden;
    }
    .snow_img {
        width:90rpx;
        height: 90rpx;
        position: absolute;
        top: -50rpx;
        margin-left: 2rpx;
        z-index: 9999;
    }

    #audio_btn {
        width: 70rpx;
        height: 70rpx;
        background: url('https://www.fuxingfuli.com/images/giftstore/Ymusic.png') no-repeat;
        background-size: 100%;
        position: fixed;
        right: 15rpx;
        top: 15rpx;
        z-index: 9999;
    }
    .rotate {
        -webkit-animation: rotating 1.2s linear infinite;
        -moz-animation: rotating 1.2s linear infinite;
        -o-animation: rotating 1.2s linear infinite;
        animation: rotating 1.2s linear infinite;
        position: relative;
    }
    #media{
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
    }
    @keyframes rotating {
        from {
            transform: rotate(0)
        }

        to {
            transform: rotate(360deg)
        }
    }
    /* 红包样式 */
    .open {
        width: 100%;
        position: fixed;
        left: 0;
        z-index: 4000;
    }

    .open_top {
        height: 56.3%;
        /* background:url(${ctx}/images/songli/top_putong.png) no-repeat; */
        background: url('https://www.fuxingfuli.com/images/giftstore/ygiftbgs.png') no-repeat;
        background-size: 100% 100%;
        top: 0;
        transition: all 1s;
        -moz-transition: all 1s;
        -webkit-transition: all 1s;
        -o-transition: all 1s;
    }

    .open_bottom {
        height: 43.8%;
        /* background:url(${ctx}/images/songli/bottom_putong.png) no-repeat; */
        background: url('https://www.fuxingfuli.com/images/giftstore/newBgyear_btn.png') no-repeat;
        background-size: 100% 100%;
        bottom: 0;
        transition: all 1s;
        -moz-transition: all 1s;
        -webkit-transition: all 1s;
        -o-transition: all 1s;
    }

    .open_bottom.on {
        transform: translateY(101%);
    }

    .open_top.on {
        transform: translateY(-101%);
    }

    .open_icon {
        width: 150rpx;
        height: 150rpx;
        position: fixed;
        z-index: 4001;
        left: 50%;
        margin-left: -75rpx;
        background:url(https://www.fuxingfuli.com/images/giftstore/openRedy.png) no-repeat;
        background-size: 100% 100%;
        top: 57.5%;
        margin-top: -75rpx;
        transform: scale(.8);
        animation-name: mychange;
        animation-duration: 1000ms;
        animation-iteration-count: infinite;
        animation-direction: alternate;
    }

    .open_icon.on {
        display: none;
    }

    .send_gift {
        padding-top: 188rpx;
        color: #fff;
        font-size: 42rpx;
        text-align: Center;
    }

    .send_giftText {
        font-size: 34rpx;
        color: #fff;
        text-align: Center;
        padding-top: 188rpx;
        opacity: 0.8;
    }

    @keyframes mychange {
        0% {
            transform: scale(.8);
            -ms-transform: scale(.8);
            -moz-transform: scale(.8);
            -webkit-transform: scale(.8);
            -o-transform: scale(.8);
        }

        100% {
            transform: scale();
            -ms-transform: scale(1);
            -moz-transform: scale(1);
            -webkit-transform: scale(1);
            -o-transform: scale(1);
        }
    }
