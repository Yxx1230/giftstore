/* pages/storeHouse/storeHouse.wxss */

.house-bar {
  padding: 0 30rpx;
  background: #fff;
  box-shadow: 2rpx 6rpx 10rpx rgba(0, 0, 0, 0.1);
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 300;
}

.house-bar-view {
  display: flex;
}

.house-bar-view>view {
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  padding: 25rpx 0;
  border-bottom: 4rpx solid transparent;
}

.con_1_ps {
  color: #f4063a;
  text-align: center;
  padding: 20rpx 0;
  font-size: 26rpx;
}

.con_1_ps text {
  padding: 0 10rpx;
  font-size: 40rpx;
}

.con-1-list, .con-2-list, .con-3-list {
  padding: 30rpx;
  padding-top: 0;
}

.con-1-list .list-item, .con-2-list .list-item, .con-3-list .list-item {
  background: #fff;
  padding: 20rpx;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 10rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 20rpx;
}

.con-1-list .list-item image {
  width: 100%;
  height: 100%;
  display: block;
}

.p_name {
  padding-top: 20rpx;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

}

.p_num {
  font-size: 24rpx;
  padding-top: 20rpx;
}

.p_num text {
  color: #f3063a;
  /* margin: 0 10rpx; */
  /* padding: 0 10rpx; */
}

.btn-group {
  text-align: right;
  margin-top: 30rpx;
}
.btn-groupe{
  text-align: right;
  margin-top: 20rpx;
}
.lookwuliu{
  display: inline-block;
  padding: 10rpx 20rpx;
  font-size: 26rpx;
  border-radius: 10rpx;
  margin-left: 20rpx;
  border: 1px solid #666;
  color: #666;
}

.givegift {
  display: inline-block;
  padding: 10rpx 20rpx;
  font-size: 26rpx;
  border-radius: 10rpx;
  border: 2rpx solid #f4063a;
  margin-left: 20rpx;
  color: #f4063a;
}

.house_con {
  padding-top: 90rpx;
}

.con_2, .con_3 {
  padding-top: 30rpx;
  display: none;
}

.con_1 {
  display: none;
}

.house_con>.active {
  display: block;
}


.item-title {
  position: relative;
  /* height: 60rpx;
  line-height: 60rpx; */
  /* margin-top: -10rpx; */
  border-bottom: 2rpx solid #eee;
  font-size: 24rpx;
  padding: 10rpx 0rpx 30rpx 0rpx;
}
.titleleft{
  float: left;
}
.titletype{
  float: right;
  color: #f4063a;
}

.con_2 .item-title text {
  display: block;
  position: absolute;
  color: #f00;
  height: 50rpx;
  line-height: 50rpx;
  right: 0;
  top: 50%;
  margin-top: -25rpx;
  font-size: 28rpx;
}

.p_dl {
  /* padding: 20rpx 0; */
  padding-top: 20rpx;
}

.p_dt {
  width: 166rpx;
  height: 90rpx;
  float: left;
  border-radius: 8rpx;
  overflow: hidden;
}

.p_dt image {
  width: 166rpx;
  height: 90rpx;
}

.pdnum {
  font-size: 24rpx;
  float: right;
  text-align: right;
}

.p_dd {
  padding-left: 22rpx;
  float: left;
  width: 370rpx;
  font-size: 24rpx;
  font-family: 'Microsoft YaHei'
}
.pdd-1{
    overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.pdnumber{
  color: #666;
  font-size: 22rpx;
  margin-top: 15rpx;
}
.pdprice{
  font-size: 26rpx;
}
.pednum {
  background: #f82654;
  color: #fff;
  padding:0 5rpx;
  width: auto;
  display: inline-block;
  font-size: 20rpx;
  margin-top: 20rpx;
}

