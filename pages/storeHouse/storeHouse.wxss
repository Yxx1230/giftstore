.list-item {
  position: relative;
}

.engrave {
  width: 180rpx;
  height: 42rpx;
  position: absolute;
  bottom: 40rpx;
  right: 45rpx;
  z-index: 100;
}

.con-3-list .item-title {
  text-align: right;
  font-size: 24rpx;
}

.con-3-list .item-title>view {
  position: absolute;
  height: 60rpx;
  line-height: 60rpx;
  top: 50%;
  margin-top: -35rpx;
  width: 230rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.con-3-list .item-title>view image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.con-3-list .item-title>view text {
  vertical-align: 15rpx;
  color: #333;
  position: static;
}

.con-3-list .p_dd-1 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  white-space: normal;
  font-size: 24rpx;
}

.con-3-list .p_dd-1>view {
  /* top:0;
  margin-top:0; */
}

.lx {
  font-size: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  float: left;
  height: 60rpx;
  color: #666;
}

.lxtext {
  width: 27rpx;
  height: 27rpx;
  margin-right: 10rpx;
  display: inline-block;
  background: url(https://www.fuxingfuli.com/images/giftstore/graykefu.png) no-repeat;
  background-size: 100% 100%;
  vertical-align: -8rpx;
}

.con_3_btn {
