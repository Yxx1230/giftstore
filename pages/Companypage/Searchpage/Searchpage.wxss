/* pages/Companypage/Searchpage/Searchpage.wxss */
/* 搜索页面 */
page {
  width: 100%;
  /* height: 100%; */
  background: #f2f2f2;
  cursor:default;
  -webkit-user-select:none;
  user-select:none;
}
.searchbox{
  width: 100%;
  height: 100%;
  background: #ffffff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 11;
}
.classifytop{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  /* border-bottom: 1px solid #dddddd; */
   box-shadow: 0px 1rpx 8rpx #cfcfcf;
}
.classifyinp{
  width: 100%;
  height: 100%;
  padding-left: 20rpx;
}
.boxsearch{
  background:#f3f3f3;
  width: 635rpx;
  height: 60rpx;
  font-size: 23rpx;
  border-radius: 30rpx;
  float: left;
}
.clboxsearch{
  width: auto;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 27rpx;
  text-align: center;
  float: right;
}
.btnboxsearch{
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #333333;
  margin-bottom: 20rpx;
  margin-top: 10rpx;
}
.csearchbtn{
  padding: 20rpx;
  box-sizing: border-box;
}
.btndelbox{
   width:28rpx;
  height:30rpx;
    background: url(https://www.fuxingfuli.com/images/giftstore/delsearch.png) no-repeat center ;
  background-size: 100%;
  display: inline-block;

}
.boxsearchitem{
  width: auto;
  padding: 14rpx 30rpx;
  box-sizing: border-box;
  background: #f3f3f3;
  font-size: 24rpx;
  text-align: center;
  float: left;
  border-radius: 30rpx;
  margin-right: 20rpx;
  margin-bottom: 30rpx;
}
.nohavefuli{
  margin-top: 30rpx;
  text-align: center;
  font-size: 24rpx;
  color: #666666;
}

/* 商品样式 */

.welfare-content{
 min-height:400rpx;
 margin-top:1rpx;
 /* border-top: 1px solid #eee; */
}
.no-data image {
  display: block;
  width: 208rpx;
  height: 207rpx;
  margin: 0 auto;
  margin-top: 200rpx;
}
.no-data text {
  display: block;
  width:100%;
  text-align: center;
  margin-top: 100rpx;
  font-size: 26rpx;
}
.welfare-item {
  width: 100%;
  padding: 30rpx 20rpx 26rpx;
  height: auto;
  overflow: hidden;
  background: #fff;
  border-bottom: 1px solid #f2f2f2;
}
.welfare-thumb {
  float: left;
  width: 268rpx;
  height: 168rpx;
  margin-right: 30rpx;
}
.welfare-thumb image {
    width: 100%;
    height: 100%;
}
.welfare-info {
  float: left;
  width: 410rpx;
}
.welfare-nametitle {
  font-size: 32rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
  width: 100%;
}
.welfare-addr{
  height:50rpx;
  line-height: 50rpx;
}
.addricon{
  /* width: 24rpx;
  height: 24rpx; */
  width: 16rpx;
  height: 22rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/adsicon.png) no-repeat center ;
  background-size: 100%;
  /* margin-right: 8rpx;
  margin-bottom: 4rpx; */
  display: inline-block;
  
}
.addrtext{
  font-size: 24rpx;
  color: #666;
  margin-left: 12rpx;
}
.welfare-price{
    height:40rpx;
  line-height: 40rpx;
}
.welfare-price-sm {
  font-size: 22rpx;
  color: #ff1616;
  margin-right: 5rpx;
}

.welfare-price-pink {
  font-size: 30rpx;
  color: #ff1616;
  margin-right: 25rpx;
}
.welfare-bought {
  font-size: 20rpx;
  color: #666;
}
.welfare-zhuan {
  font-size: 20rpx;
  color: #fc4f69;
  border: 1px solid #fc4f69;
  padding: 0 6rpx;
  float: right;
  margin-top: 14rpx;
}