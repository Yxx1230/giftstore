/**index.wxss**/

page {
  width: 100%;
  /* height: 100%; */
  background: #f3f3f3;
  cursor: default;
  -webkit-user-select: none;
  user-select: none;
}

.headernav {
  width: 100%;
  background: #fff;
  padding: 20rpx 32rpx;
  box-sizing: border-box;
  box-shadow: 0px 1rpx 8rpx #cfcfcf;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}

.giftlogo {
  width: 130rpx;
  height: 50rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/giftlogo.png) no-repeat;
  background-size: 100% 100%;
  display: block;
}

.rightsec {
  height: 48rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 8rpx;
  font-size: 24rpx;
}

.yuserphotos {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
  border-radius: 50%;
}

.redcolor {
  color: #f4063a;
  font-size: 24rpx;
}

.ykefus {
  width: 34rpx;
  height: 34rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/ykefus.png) no-repeat;
  background-size: 100%;
  display: block;
  margin: 0 auto;
}

.kefubox {
  width: 74rpx;
  height: 36rpx;
  line-height: 36rpx;
  border-left: 1rpx solid #ddd;
  border-right: 1rpx solid #ddd;
  text-align: center;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.ordertext {
  position: relative;
  width: 80rpx;
}

.ordernum {
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  border-radius: 50%;
  background: #fe325e;
  color: #fff;
  font-size: 18rpx;
  text-align: center;
  position: absolute;
  top: -20rpx;
  right: 0;
}

.goodshoplist {
  margin-top: 100rpx;
  padding: 30rpx;
  box-sizing: border-box;
  margin-bottom: 10rpx;
}

.contentnav {
  width: 100%;
  margin-bottom: 30rpx;
}

.yegoodshopes {
  width: 380rpx;
  height: 40rpx;
  margin: 0 auto;
  background: url(https://www.fuxingfuli.com/images/giftstore/ygoodshops.png) no-repeat;
  background-size: 100%;
}

.ygoodshops {
  width: 380rpx;
  height: 40rpx;
  margin: 0 auto;
  background: url(https://www.fuxingfuli.com/images/giftstore/ygoodshope.png) no-repeat;
  background-size: 100%;
}

/* 店铺列表 */

.company-info-item {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding: 30rpx 20rpx 16rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
}

.company-info {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.company-thumb {
  width: 100rpx;
  height: 100rpx;
  /* background: #fff; */
  box-shadow: #d4d3d3 0rpx 0rpx 10rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
}

.company-thumb image {
  width: 100%;
  height: 100%;
}

.name-authentication {
  flex: 1;
}

.company-name {
  font-size: 28rpx;
  color: #000;
}

.renzheng {
  width: 132rpx;
  height: 30rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/renzheng.png) no-repeat;
  background-size: 100%;
  display: inline-block;
}

.yqilogo {
  width: 24rpx;
  height: 26rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/yqilogo.png) no-repeat;
  background-size: 100%;
  display: inline-block;
}

.yyelogo {
  width: 22rpx;
  height: 26rpx;
  background: url(https://www.fuxingfuli.com/images/giftstore/yyelogo.png) no-repeat;
  background-size: 100%;
  display: inline-block;
}

.look {
  width: 120rpx;
  height: 46rpx;
  border: 1rpx solid #f4063a;
  color: #f4063a;
  font-size: 22rpx;
  text-align: center;
  line-height: 44rpx;
  border-radius: 24rpx;
}

.recommend-scroll {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  /* margin-left: 28rpx 0; */
  white-space: nowrap;
  padding-top: 28rpx;
  /* padding-left: 20rpx; */
}

.recommend-scroll .item-view {
  display: inline-block;
  width: 270rpx;
  height: auto;
  overflow: hidden;
  border: 1px solid #ddd;
  border-radius: 10rpx;
  margin-right: 20rpx;
  padding-bottom: 16rpx;
}

.recommend-scroll .item-view:last-child {
  margin-right: 0rpx;
}

.recommend-thumb {
  width: 100%;
  height: 170rpx;
  border-radius: 10rpx 10rpx 0 0;
}

.goodsInfo {
  width: 100%;
  padding: 0 10rpx;
}

.goodsName {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 24rpx;
  color: #333;
}

.bought {
  color: #999;
  font-size: 20rpx;
  float: right;
  margin-top: 15rpx;
}

.zhuan {
  width: auto;
  height: 26rpx;
  color: #fc4f69;
  border: 1px solid #fc4f69;
  font-size: 18rpx;
  padding: 0 10rpx;
  line-height: 24rpx;
}

.price {
  font-size: 34rpx;
  color: #ff1616;
}

.repricesm {
  color: #f94444;
  font-size: 34rpx;
}

.priceleft {
  display: flex;
  align-items: center;
}

.pricesm {
  font-size: 24rpx;
  margin-right: 12rpx;
}

.pricesmes {
  font-size: 24rpx;
  margin-right: 5rpx;
}

/* 推荐福利 */

.recommendlist {
  width: 100%;
}

.recommendinfo {
  width: 330rpx;
  height: auto;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 10rpx;
  border: 1px solid #ddd;
  float: left;
  box-shadow: 1px 1px 6px #ddd;
  margin-bottom: 20rpx;
}

.recommendimg {
  width: 100%;
  height: 155rpx;
  border-radius: 10rpx;
}

.recommendinfo:nth-child(odd) {
  margin-right: 30rpx;
}

.retitle {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 24rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

/* 底部导航栏 */

.footernav {
  width: 100%;
}
